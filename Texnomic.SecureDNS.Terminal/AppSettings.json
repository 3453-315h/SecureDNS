{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://127.0.0.1:5341",
          "compact": true
        }
      }
    ],
    "Properties": {
      "Application": "SecureDNS"
    }
  },

  "HTTPs Protocol": {
    "Uri": "https://dns.google/",
    "PublicKey": null,
    "AllowRedirects": false,
    "Retries": 3,
    "WebProxy": null
  },

  "TLS Protocol": {
    "Host": "dns.google",
    "Port": 853,
    "Timeout": 2000,
    "PublicKey": null
  },

  "Proxy Responsibility Chain": {
    "Middlewares": [
      "HostTableMiddleware",
      "FilterListsMiddleware",
      "ResolverMiddleware"
    ]
  },

  "Terminal Options": {
    "Mode": "GUI"
  },

  "Proxy Server": {
    "Address": "127.0.0.1",
    "Port": 53
    //"Threads": 2
  },

  "HostTable Middleware": {
    "TimeToLive": 30,
    "HostTable": {
      "dns.google": "8.8.8.8",
      "one.one.one.one": "1.1.1.1"
    }
  },

  "FilterLists Middleware": {
    "IDs": [
      10,
      685
    ]
  }
}