{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "materialize-stepper@3.1.0",
      "destination": "wwwroot/lib/materialize-stepper/",
      "files": [
        "dist/css/mstepper.min.css",
        "dist/js/mstepper.min.js"
      ]
    },
    {
      "library": "materialize@1.0.0",
      "destination": "wwwroot/lib/materialize/",
      "files": [
        "css/materialize.min.css",
        "css/materialize.css",
        "js/materialize.min.js"
      ]
    },
    {
      "library": "material-design-icons@3.0.1",
      "destination": "wwwroot/lib/material-design-icons/",
      "files": [
        "iconfont/material-icons.min.css",
        "iconfont/MaterialIcons-Regular.eot",
        "iconfont/MaterialIcons-Regular.svg",
        "iconfont/MaterialIcons-Regular.ttf",
        "iconfont/MaterialIcons-Regular.woff",
        "iconfont/MaterialIcons-Regular.woff2"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "library": "font-awesome@5.10.2",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/fa-brands-400.eot",
        "webfonts/fa-brands-400.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.eot",
        "webfonts/fa-regular-400.svg",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.eot",
        "webfonts/fa-solid-900.svg",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2",
        "js/all.min.js"
      ]
    },
    {
      "library": "popper.js@1.15.0",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "popper.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "Assets/Blazorise/",
      "destination": "wwwroot/lib/Blazorise/",
      "files": [
        "blazorise.css",
        "blazorise.bootstrap.css",
        "blazorise.sidebar.css",
        "blazorise.snackbar.css",
        "blazorise.js",
        "blazorise.charts.js"
      ]
    }
  ]
}